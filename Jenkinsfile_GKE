             curl -k -d '{"application": "engangsstonad", "version": "latest", "fasitEnvironment": "t1", "zone": "sbs", "fasitUsername": "${USERNAME}", "fasitPassword": "${PASSWORD}", "namespace": "default", "manifesturl": "https://raw.githubusercontent.com/navikt/engangsstonad/master/nais.yaml?token=${token}"}' http://35.187.46.35/deploy
        }
    }

